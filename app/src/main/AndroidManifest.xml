<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pe.edu.upc.municipalidadcallao">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DetalleCarro"
            android:label="@string/title_activity_detalle_carro"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ListadoDeclaracionesActivity"
            android:label="@string/title_activity_listado_declaraciones"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DetalleDeclaracionActivity"
            android:label="@string/title_activity_detalle_declaracion"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EstadoCuentaActivity"
            android:label="@string/title_activity_estado_cuenta"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>